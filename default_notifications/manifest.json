{
  "manifest_version": 3,

  "name": "Default Notifications",
  "version": "8",
  "description": "Adds some common pages to the allowed extensions list.",

  "options_page": "options.html",

  "permissions": [
    "contentSettings",
    "storage"
  ],

  "icons": {
    "128": "icon128.png"
  },

  "background": {
    "service_worker": "worker.js",
    "type": "module"
  }
}
